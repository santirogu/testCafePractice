{
  "name": "testcafepractice",
  "version": "1.0.0",
  "description": "This is a test cafe automation framework practice.",
  "main": "index.js",
  "scripts": {
    "debug": "node --inspect=1337 --debug-brk --nolazy node_modules/cucumber/bin/cucumber-js --tags @debug --format json:./reports/report.json",
    "chrome": "testcafe chrome tests/ --reporter html:reports/report.html",
    "chromeheadless": "testcafe chrome:headless tests/ --reporter html:reports/report.html",
    "firefox": "testcafe firefox tests/ --reporter html:reports/report.html",
    "firefoxheadless": "testcafe firefox:headless tests/ --reporter html:reports/report.html",
    "edge": "testcafe edge tests/ --reporter html:reports/report.html",
    "all": "testcafe all tests/ --reporter html:reports/report.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/santirogu/testCafePractice.git"
  },
  "keywords": [
    "testcafe"
  ],
  "author": "Santiago RG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/santirogu/testCafePractice/issues"
  },
  "homepage": "https://github.com/santirogu/testCafePractice#readme",
  "devDependencies": {
    "testcafe": "^1.8.1"
  },
  "dependencies": {
    "cucumber": "^6.0.5",
    "gherkin-testcafe": "^2.5.0",
    "testcafe-reporter-html": "^1.4.6"
  },
  "types": "index.d.ts"
}
